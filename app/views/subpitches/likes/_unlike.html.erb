<% (subpitch = @subpitch) if @subpitch.present? %>
<%= form_for liked, url: pitch_subpitch_like_path(params[:pitch_id], subpitch.id, check_like(subpitch).ids.join().to_i), method: :delete, remote: true do |f| %>
  <%= link_to "#", class: "btn btn-info btn-lg like_subpitch", style: "width: 100px;", onclick: "$('#edit_like_#{liked.id}').submit();" do %>
    <span class="glyphicon glyphicon-thumbs-down"></span>
  <% end %>
<% end %>
